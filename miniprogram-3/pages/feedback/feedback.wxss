page{
  background-color: #eee;
}
.fad_main{
  padding:20rpx;
  color:#666;
}
.fad_main .f_m_tilte{  
}
.fad_main .f_m_tips{
  display: flex;
  flex-wrap: wrap;
}
.fad_main .f_m_tips text{
    width: 30%;
    padding:10rpx 0;
    text-align: center;
    background-color: #fff;
    margin:20rpx 10rpx;
}
.fad_main .f_m_content{
  background-color: #fff;
  margin-top:20rpx;
}
.fad_main .f_m_content textarea{
    padding:20rpx;
}
.fad_main .f_m_content .f_m_tool{
   display: flex;
   flex-wrap: wrap;
   padding-bottom: 30rpx;
}
.fad_main .f_m_content .f_m_tool button{
  
  width: 90rpx;
  height:90rpx;
  font-size:60rpx;
  padding:0;
  margin:0;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left:20rpx;
  margin-top:20rpx;
  color:#bbb;
}

.fad_main .f_m_content .f_m_tool .up_img_list{
   margin-left:20rpx;
   margin-top:20rpx;
}

.fad_main .form_btn_wrap{
   margin-top:20rpx;
   display: flex;
   justify-content: flex-end;
}
.fad_main .form_btn_wrap button{
    margin:0;
    width: 30%;
}
.fad_main .form_btn_wrap icon{
    vertical-align: middle;
    margin-right: 3rpx;
}